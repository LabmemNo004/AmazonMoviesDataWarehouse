select movieID, title, productNum from movie
where title='Harry Potter and the Goblet of Fire'
   or title='Harry Potter and the Chamber of Secrets';
   //
   6902,Harry Potter and the Chamber of Secrets,2
   16272,Harry Potter and the Chamber of Secrets,1
